{
    "extends": ["airbnb", "prettier", "prettier/react"],
    "plugins": ["prettier"],
    "rules": {
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [".js", ".jsx"]
        }
      ],
      "react/prop-types": 0,
      "no-underscore-dangle": 0,
      "import/imports-first": ["error", "absolute-first"],
      "import/newline-after-import": "error",
      "import/prefer-default-export": 0,
      "import/no-unresolved": "off",
      "no-console":"off",
      "react/no-array-index-key":"off",
      "semi": "error",
      "spaced-comment":"off",
      "no-unused-vars":"off",
      "no-param-reassign":"off"
    },
    "globals": {
      "window": true,
      "document": true,
      "localStorage": true,
      "FormData": true,
      "FileReader": true,
      "Blob": true,
      "navigator": true,
      "Headers": true,
      "Request": true,
      "fetch": true
    },
    "parser": "babel-eslint"
  }